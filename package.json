{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@auto-it/all-contributors": "^11.0.0",
    "@auto-it/conventional-commits": "^11.0.0",
    "@auto-it/first-time-contributor": "^11.0.0",
    "@auto-it/released": "^11.0.0",
    "@turbo/gen": "^1.9.7",
    "auto": "^11.0.0",
    "eslint": "^7.32.0",
    "eslint-config-custom": "workspace:*",
    "prettier": "^2.5.1",
    "turbo": "latest"
  },
  "packageManager": "pnpm@8.6.10",
  "name": "my-turborepo",
  "repository": "betoobernal/turborepo",
  "author": "Elkin Bernal <elkinbernal21@gmail.com>",
  "auto": {
    "plugins": [
      "npm",
      "all-contributors",
      "conventional-commits",
      "first-time-contributor",
      "released"
    ]
  }
}